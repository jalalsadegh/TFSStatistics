<Window x:Class="TFSStatistics.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TFSStatistics"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="button" Content="Calculate" HorizontalAlignment="Left" Margin="10,81,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <TextBox x:Name="UserName" HorizontalAlignment="Left" Height="23" Margin="108,73,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label" Content="user" HorizontalAlignment="Left" Margin="108,42,0,0" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label_Copy" Content="password&#xD;&#xA;" HorizontalAlignment="Left" Margin="108,116,0,0" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.504,0.11" Height="29"/>
        <TextBox x:Name="password" HorizontalAlignment="Left" Height="23" Margin="108,150,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
